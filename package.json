    "file-loader": "^6.2.0",
    "html-loader": "^1.3.2",
    "html-webpack-plugin": "^4.5.0",
    "husky": "^4.3.0",
    "jest": "^26.6.3",
    "jest-environment-jsdom": "^26.6.2",
    "jsdom": "^16.4.0",
    "less": "^3.12.2",
    "less-loader": "^7.0.2",
    "lint-staged": "^10.5.1",
    "mini-css-extract-plugin": "^1.3.0",
    "open-browser-webpack-plugin": "0.0.5",
    "optimize-css-assets-webpack-plugin": "^5.0.4",
    "postcss": "^8.1.6",
    "postcss-loader": "^4.0.4",
    "power-assert": "^1.6.1",
